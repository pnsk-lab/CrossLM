{
  "name": "@pnsk-lab/crosslm",
  "version": "0.2.4",
  "exports": {
    ".": "./src/mod.ts",

    "./cohere/command-r-plus": "./src/models/cohere/command-r-plus.ts",
    "./cohere/command-r": "./src/models/cohere/command-r.ts",
    "./cohere/command": "./src/models/cohere/command.ts",

    "./groq": "./src/models/groq/groq.ts",

    "./gemini/15-flash": "./src/models/gemini/15-flash.ts",
    "./gemini/15-pro": "./src/models/gemini/15-pro.ts"
  },
  "tasks": {
    "d": "deno run -A .idea/main.ts",
    "build:npm": "deno run -A scripts/npm.ts",
    "npm": "deno task build:npm && cd npm && npm publish"
  },
  "fmt": {
    "semiColons": false,
    "singleQuote": true
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.0",
    "@std/path": "jsr:@std/path@^1.0.1"
  },
  "publish": {
    "exclude": [
      "./.vscode/**/*",
      "./**/*.test.ts",
      "./scripts/**/*",
      "./npm/**/*"
    ]
  }
}
